{
   "compileOnSave": false,
   "compilerOptions": {
      "baseUrl": "./",
      "outDir": "./dist/out-tsc",
      "sourceMap": true,
      "declaration": false,
      "module": "esnext",
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "incremental": true,
      "target": "es2015",
      "typeRoots": [
         "node_modules/@types"
      ],
      "lib": [
         "es2018",
         "dom"
      ],
      "paths": {
         "egeo": [
            "dist/egeo"
         ],
         "egeo/*": [
            "dist/egeo/*"
         ],
         "@app/shared/*": [
            "./projects/sds-demo/src/app/shared/*"
         ],
         "@app/demos/*": [
            "./projects/sds-demo/src/app/demos/*"
         ],
         "@stratio/egeo": [
            "./projects/egeo/src/public_api.ts"
         ]
      }
   }
}
